Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 

  escreva("coloque o valor de a1")
  leia(a1)
  escreva("coloque o valor de f2")
  leia(f2)
  escreva("coloque o valor de m3")
  leia(m3)
  escreva("coloque o valor de c4")
  leia(c4)

  se(a1=9.00) entao
  escreval("na media")
  senao
  Escreval("abaixo da media")
  Fimse

  se(f2=12.00) entao
  escreval("na media")
  senao
  Escreval("abaixo da media")
  Fimse

  se(m3=5.00) entao
  escreval("na media")
  senao
  Escreval("abaixo da media")
  Fimse

  se(c4=30.00) entao
